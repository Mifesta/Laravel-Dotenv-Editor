{
    "name": "mifesta/dotenv-editor",
    "version": "1.0.0",
    "description": "The .env file editor tool for Laravel 5+",
    "license": "MIT",
    "keywords": [
        "laravel",
        "dotenv",
        "dotenv-editor"
    ],
    "authors": [
        {
            "name": "Mifesta",
            "email": "dev@mifesta.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": ">=5.0",
        "illuminate/container": ">=5.0",
        "illuminate/support": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Mifesta\\DotenvEditor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
